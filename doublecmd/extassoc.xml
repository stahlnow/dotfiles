<?xml version="1.0" encoding="utf-8"?>
<doublecmd DCVersion="0.7.8 beta">
  <ExtensionAssociation>
    <FileType>
      <Name>Images</Name>
      <IconFile>~/sh/rifle_sxiv.sh</IconFile>
      <ExtensionList>jpg|jpeg|png|gif|tiff|tif|bmp</ExtensionList>
      <Actions>
        <Action>
          <Name>View</Name>
          <Command>~/sh/rifle_sxiv.sh</Command>
          <Params>%f &amp;&amp; wmctrl -a sxiv %&gt; /dev/null</Params>
        </Action>
        <Action>
          <Name>Open</Name>
          <Command>~/sh/rifle_sxiv.sh</Command>
          <Params>%f &amp;&amp; wmctrl -a sxiv %&gt; /dev/null</Params>
        </Action>
        <Action>
          <Name>Edit</Name>
          <Command>gimp</Command>
          <Params>%p</Params>
        </Action>
      </Actions>
    </FileType>
    <FileType>
      <Name>SVG</Name>
      <IconFile/>
      <ExtensionList>svg</ExtensionList>
      <Actions>
        <Action>
          <Name>View</Name>
          <Command>~/sh/feh_browser.sh</Command>
          <Params>%fs -F -d -q --draw-tinted</Params>
        </Action>
        <Action>
          <Name>Edit</Name>
          <Command>inkscape</Command>
          <Params>%p</Params>
        </Action>
      </Actions>
    </FileType>
    <FileType>
      <Name>Wav</Name>
      <IconFile/>
      <ExtensionList>wav</ExtensionList>
      <Actions>
        <Action>
          <Name>View</Name>
          <Command>aplay</Command>
          <Params>%p</Params>
        </Action>
        <Action>
          <Name>Edit</Name>
          <Command>audacity</Command>
          <Params>%p</Params>
        </Action>
      </Actions>
    </FileType>
    <FileType>
      <Name>sfv</Name>
      <IconFile/>
      <ExtensionList>sfv</ExtensionList>
      <Actions>
        <Action>
          <Name>Open</Name>
          <Command>{!SHELL} cksfv</Command>
          <Params>-f %p</Params>
        </Action>
        <Action>
          <Name>View</Name>
          <Command>{!SHELL} cksfv</Command>
          <Params>-f %p</Params>
        </Action>
      </Actions>
    </FileType>
    <FileType>
      <Name>mp3,m3u</Name>
      <IconFile/>
      <ExtensionList>mp3|m3u|flac</ExtensionList>
      <Actions>
        <Action>
          <Name>Open</Name>
          <Command>deadbeef</Command>
          <Params>--queue %p</Params>
        </Action>
        <Action>
          <Name>View</Name>
          <Command>deadbeef</Command>
          <Params>--queue %p</Params>
        </Action>
      </Actions>
    </FileType>
    <FileType>
      <Name>pdf</Name>
      <IconFile/>
      <ExtensionList>pdf</ExtensionList>
      <Actions>
        <Action>
          <Name>Edit</Name>
          <Command>inkscape</Command>
          <Params>%p</Params>
        </Action>
      </Actions>
    </FileType>
    <FileType>
      <Name>eps</Name>
      <IconFile/>
      <ExtensionList>eps</ExtensionList>
      <Actions>
        <Action>
          <Name>Edit</Name>
          <Command>inkscape</Command>
          <Params>%p</Params>
        </Action>
      </Actions>
    </FileType>
    <FileType>
      <Name>video</Name>
      <IconFile/>
      <ExtensionList>mkv|mp4|avi|mpeg</ExtensionList>
      <Actions>
        <Action>
          <Name>Open</Name>
          <Command>mpv</Command>
          <Params>%p</Params>
        </Action>
      </Actions>
    </FileType>
  </ExtensionAssociation>
</doublecmd>
