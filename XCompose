# -*- coding: utf-8; mode: conf -*-
include "%L"

# german diaresis a.k.a. umlaut
<Multi_key> <u> <a>: "ä"
<Multi_key> <u> <A>: "Ä"
<Multi_key> <u> <o>: "ö"
<Multi_key> <u> <O>: "Ö"
<Multi_key> <u> <u>: "ü"
<Multi_key> <u> <U>: "Ü"

<Multi_key> <-> <->: "—"

# some greek letters
<Multi_key> <g> <a> : "α"       # Alpha (ἄλφα)
<Multi_key> <g> <A> : "Α"       #
<Multi_key> <g> <b> : "β"       # Beta (βῆτα)
<Multi_key> <g> <B> : "Β"       #
<Multi_key> <g> <g> : "γ"       # Gamma (γάμμα)
<Multi_key> <g> <G> : "Γ"       #
<Multi_key> <g> <d> : "δ"       # Delta (δέλτα)
<Multi_key> <g> <D> : "Δ"       #
<Multi_key> <g> <e> : "ε"       # Epsilon (ἔ ψιλόν)
<Multi_key> <g> <E> : "Ε"       #
<Multi_key> <g> <z> : "ζ"       # Zeta (ζῆτα)
<Multi_key> <g> <z> : "Ζ"       #
<Multi_key> <g> <t> <h> : "θ"   #
<Multi_key> <g> <T> <h> : "Θ"
<Multi_key> <g> <l> : "λ"       # Lambda (λάμβδα)
<Multi_key> <g> <L> : "Λ"
<Multi_key> <g> <p> : "π"       # Pi (πῖ)
<Multi_key> <g> <P> : "Π"       #
<Multi_key> <g> <r> <h> : "ρ"   # Rho (ῥῶ)
<Multi_key> <g> <R> <h> : "Ρ"   #
<Multi_key> <g> <s> : "σ"       # Sigma (σῖγμα)
<Multi_key> <g> <S> : "Σ"       #
<Multi_key> <g> <t> : "τ"       # Tau (ταῦ)
<Multi_key> <g> <T> : "Τ"       #
<Multi_key> <g> <y> : "υ"       # Ypsilon (ὔ ψιλόν)
<Multi_key> <g> <Y> : "Υ"       #
<Multi_key> <g> <p> <h> : "φ"   # Phi (φῖ)
<Multi_key> <g> <P> <h> : "Φ"   #
<Multi_key> <g> <p> <s> : "ψ"   # Psi (ψῖ)
<Multi_key> <g> <P> <s> : "Ψ"   #
<Multi_key> <g> <o> : "ω"       # Omega (ὦ μέγα)
<Multi_key> <g> <O> : "Ω"       #
